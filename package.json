{
  "name": "asu-web-standards-bootstrap4-prototype",
  "version": "1.0.0-4.3",
  "version_short": "4.3",
  "description": "Preliminary scaffolding for the new ASU web Standards Bootstrap 4 project",
  "style": "dist/css/bootstrap-asu.css",
  "sass": "src/scss/bootstrap-asu.scss",
  "main": "dist/js/bootstrap-asu.js",
  "module": "dist/js/bootstrap-asu.esm.js",
  "repository": "https://github.com/gios-asu/asu-web-standards-bootstrap4-prototype.git",
  "homepage": "https://getbootstrap.com/",
  "author": "Nathan Rollins <nathan.rollins@asu.edu>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "npm-run-all --parallel watch docs-serve",
    "bundlesize": "bundlesize",
    "css": "npm-run-all css-compile css-prefix css-minify css-copy",
    "css-copy": "cross-env-shell shx mkdir -p site/static/docs/$npm_package_version_short/dist/ && cross-env-shell shx cp -r dist/css/ site/static/docs/$npm_package_version_short/dist/",
    "css-main": "npm-run-all css-lint css-compile-main css-prefix-main css-minify-main css-copy",
    "css-docs": "npm-run-all css-compile-docs css-prefix-docs* css-minify-docs",
    "css-compile": "npm-run-all --parallel css-compile-*",
    "css-compile-main": "node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 src/scss/ -o dist/css/ && npm run css-copy",
    "css-compile-docs": "cross-env-shell node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 site/static/docs/$npm_package_version_short/assets/scss/docs.scss site/static/docs/$npm_package_version_short/assets/css/docs.min.css",
    "css-lint": "npm-run-all --continue-on-error --parallel css-lint-*",
    "css-lint-main": "stylelint \"src/scss/**/*.scss\" --cache --cache-location .cache/.stylelintcache",
    "css-lint-docs": "stylelint \"site/static/**/assets/scss/*.scss\" \"site/content/**/*.css\" --cache --cache-location .cache/.stylelintcache",
    "css-lint-vars": "fusv src/scss/ site/static/",
    "css-minify": "npm-run-all --parallel css-minify-*",
    "css-minify-main": "cleancss --level 1 --format breakWith=lf --source-map --source-map-inline-sources --output dist/css/bootstrap-asu.min.css dist/css/bootstrap-asu.css && cleancss --level 1 --format breakWith=lf --source-map --source-map-inline-sources --output dist/css/bootstrap-grid.min.css dist/css/bootstrap-grid.css && cleancss --level 1 --format breakWith=lf --source-map --source-map-inline-sources --output dist/css/bootstrap-utilities.min.css dist/css/bootstrap-utilities.css && cleancss --level 1 --format breakWith=lf --source-map --source-map-inline-sources --output dist/css/bootstrap-reboot.min.css dist/css/bootstrap-reboot.css",
    "css-minify-docs": "cross-env-shell cleancss --level 1 --format breakWith=lf --source-map --source-map-inline-sources --output site/static/docs/$npm_package_version_short/assets/css/docs.min.css site/static/docs/$npm_package_version_short/assets/css/docs.min.css",
    "css-prefix": "npm-run-all --parallel css-prefix-*",
    "css-prefix-main": "postcss --config build/postcss.config.js --replace \"dist/css/*.css\" \"!dist/css/*.min.css\"",
    "css-prefix-docs": "cross-env-shell postcss --config build/postcss.config.js --replace \"site/static/docs/$npm_package_version_short/assets/css/*.css\"",
    "css-prefix-docs-examples": "postcss --config build/postcss.config.js --replace \"site/content/**/*.css\"",
    "js": "npm-run-all js-compile js-minify js-copy",
    "js-copy": "cross-env-shell shx mkdir -p site/static/docs/$npm_package_version_short/dist/ && cross-env-shell shx cp -r dist/js/ site/static/docs/$npm_package_version_short/dist/",
    "js-main": "npm-run-all js-lint js-compile js-minify-main",
    "js-docs": "npm-run-all js-lint-docs js-minify-docs",
    "js-compile": "npm-run-all --parallel js-compile-* --sequential js-copy",
    "js-compile-standalone": "rollup --environment BUNDLE:false --config build/rollup.config.js --sourcemap",
    "js-compile-standalone-esm": "rollup --environment ESM:true,BUNDLE:false --config build/rollup.config.js --sourcemap",
    "js-compile-bundle": "rollup --environment BUNDLE:true --config build/rollup.config.js --sourcemap",
    "js-compile-plugins": "node build/build-plugins.js",
    "js-lint": "npm-run-all --continue-on-error --parallel js-lint-*",
    "js-lint-main": "eslint --cache --cache-location .cache/.eslintcache src/js/",
    "js-lint-docs": "eslint --cache --cache-location .cache/.eslintcache site/",
    "js-minify": "npm-run-all --parallel js-minify-main js-minify-docs",
    "js-minify-main": "npm-run-all js-minify-standalone* js-minify-bundle",
    "js-minify-standalone": "terser --compress typeofs=false --mangle --comments \"/^!/\" --source-map \"content=dist/js/bootstrap-asu.js.map,includeSources,url=bootstrap-asu.min.js.map\" --output dist/js/bootstrap-asu.min.js dist/js/bootstrap-asu.js",
    "js-minify-standalone-esm": "terser --compress --mangle --comments \"/^!/\" --source-map \"content=dist/js/bootstrap-asu.esm.js.map,includeSources,url=bootstrap-asu.esm.min.js.map\" --output dist/js/bootstrap-asu.esm.min.js dist/js/bootstrap-asu.esm.js",
    "js-minify-bundle": "terser --compress typeofs=false --mangle --comments \"/^!/\" --source-map \"content=dist/js/bootstrap-asu.bundle.js.map,includeSources,url=bootstrap-asu.bundle.min.js.map\" --output dist/js/bootstrap-asu.bundle.min.js dist/js/bootstrap-asu.bundle.js",
    "js-minify-docs": "cross-env-shell terser --mangle --comments \\\"/^!/\\\" --output site/static/docs/$npm_package_version_short/assets/js/docs.min.js site/static/docs/$npm_package_version_short/assets/js/vendor/anchor.min.js site/static/docs/$npm_package_version_short/assets/js/vendor/clipboard.min.js site/static/docs/$npm_package_version_short/assets/js/vendor/bs-custom-file-input.min.js \"site/static/docs/$npm_package_version_short/assets/js/src/*.js\"",
    "lint": "npm-run-all --parallel js-lint css-lint",
    "docs": "npm-run-all css-docs js-docs docs-build docs-lint",
    "predocs-build": "npm-run-all css-copy js-copy",
    "docs-build": "hugo --cleanDestinationDir",
    "docs-compile": "npm run docs-build",
    "docs-production": "cross-env HUGO_ENV=production npm run docs-build",
    "docs-lint": "npm-run-all --parallel docs-vnu",
    "docs-vnu": "node build/vnu-jar.js",
    "predocs-serve": "npm-run-all css-copy js-copy",
    "docs-serve": "hugo server --port 9001 --disableFastRender",
    "docs-serve-only": "serve _gh_pages --listen 9001",
    "happo": "happo",
    "happo-ci-travis": "happo-ci-travis",
    "jest:integration": "jest -c jest.config.js --maxWorkers=2",
    "test:integration": "start-server-and-test storybook http-get://localhost:9009 jest:integration",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json || true",
    "prebuild:storybook": "yarn run test:generate-output",
    "storybook": "start-storybook -p 9009",
    "build-storybook": "build-storybook",
    "watch": "npm-run-all --parallel watch-*",
    "watch-css-main": "nodemon --watch src/scss/ --ext scss --exec \"npm run css-main\"",
    "watch-css-docs": "nodemon --watch \"site/static/**/assets/scss/\" --ext scss --exec \"npm run css-docs\"",
    "watch-js-main": "nodemon --watch js/src/ --ext js --exec \"npm run js-compile\"",
    "watch-js-docs": "nodemon --watch \"site/static/**/assets/js/src/\" --ext js --exec \"npm run js-docs\""
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@fortawesome/fontawesome-free": "^5.9.0",
    "@storybook/addon-a11y": "^5.1.9",
    "@storybook/addon-google-analytics": "^5.1.9",
    "@storybook/addon-jest": "^5.1.9",
    "@storybook/addon-viewport": "^5.1.9",
    "@storybook/html": "^5.1.9",
    "autoprefixer": "^9.5.1",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-istanbul": "^5.2.0",
    "bootstrap": "^4.3.1",
    "bundlesize": "^0.18.0",
    "clean-css-cli": "^4.3.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "eslint": "^6.1.0",
    "eslint-config-xo": "^0.26.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-unicorn": "^10.0.0",
    "find-unused-sass-variables": "^0.4.1",
    "fastclick": "^1.0.6",
    "glob": "^7.1.4",
    "happo-plugin-storybook": "^2.4.0",
    "happo.io": "^3.17.0",
    "hugo-bin": "^0.44.3",
    "jest": "^24.8.0",
    "jest-image-snapshot": "^2.8.2",
    "jest-puppeteer": "^4.2.0",
    "jquery": "^3.4.1",
    "linkinator": "^1.5.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "popper.js": "^1.15.0",
    "postcss-cli": "^6.1.3",
    "prettier": "^1.17.0",
    "puppeteer": "^1.17.0",
    "rollup": "^1.18.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-istanbul": "^2.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "sass-lint": "^1.13.1",
    "sass-lint-auto-fix": "^0.17.0",
    "sass-loader": "^7.1.0",
    "shx": "^0.3.2",
    "smooth-scroll": "https://github.com/cferdinandi/smooth-scroll/archive/v10.3.1.tar.gz",
    "start-server-and-test": "^1.9.1",
    "storybook-readme": "^5.0.5",
    "stylelint": "^10.1.0",
    "stylelint-config-twbs-bootstrap": "^0.4.0",
    "style-loader": "^0.23.1",
    "terser": "^4.1.2",
    "vnu-jar": "19.6.7",
    "webfontloader": "^1.6.28"
  },
  "engines": {
    "node": ">=8"
  },
  "files": [
    "dist/{css,js}/*.{css,js,map}",
    "js/{src,dist}/**/*.{js,map}",
    "!js/src/**/*.spec.js",
    "scss/**/*.scss"
  ]
}
